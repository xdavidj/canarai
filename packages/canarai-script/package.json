{
  "name": "@canarai/script",
  "version": "0.1.0",
  "description": "Embeddable AI agent detection and canar.ai test script",
  "type": "module",
  "main": "dist/canarai.js",
  "files": ["dist"],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c --watch",
