  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@vitest/coverage-v8": "^3.0.0",
    "jsdom": "^25.0.0",
    "rollup": "^4.30.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.0"
  }
}
